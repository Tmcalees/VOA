      <TableDataSource Name="Table2" Alias="CustomerReceiptMerchandiseNotes" DataType="System.Int32" Enabled="true" SelectCommand="SELECT V.MerchandiseNotes&#13;&#10;FROM &quot;VOA_Donations&quot; V&#13;&#10;WHERE V.Id = @donation_id&#13;&#10;">
        <Column Name="MerchandiseNotes" DataType="System.String"/>
