        <Column Name="Address" DataType="System.String"/>
        <Column Name="CSZ" DataType="System.String"/>
        <Column Name="Telephone" DataType="System.String"/>
        <Column Name="Date_Scheduled" DataType="System.DateTime"/>
        <Column Name="Route" DataType="System.Int32"/>
        <Column Name="Id" DataType="System.Int32"/>
        <CommandParameter Name="@pickup_date" DataType="15" Expression="[PickupDate]" DefaultValue="02-17-2009"/>
        <CommandParameter Name="@route" DataType="22" Expression="[Route]" DefaultValue="0"/>
      </TableDataSource>
      <TableDataSource Name="Table1" Alias="CustomerReceiptDonationsDetails" DataType="System.Int32" Enabled="true" SelectCommand="SELECT ItemName = &#13;&#10;   CASE V1.Merchandise_Id&#13;&#10;      WHEN 0 THEN V1.OtherMerch&#13;&#10;      ELSE V2.Name&#13;&#10;   END, V1.Qty&#13;&#10;FROM VOA_Donation_Details V1, VOA_Merchandise V2&#13;&#10;WHERE V1.Merchandise_Id = V2.Id AND V1.Donation_Id = @donation_id&#13;&#10;ORDER BY ItemName Asc">
        <Column Name="Qty" DataType="System.Int32"/>
        <Column Name="ItemName" DataType="System.String"/>
        <CommandParameter Name="@donation_id" DataType="8" Expression="[CustomerReceiptDonations.Id]" DefaultValue="0"/>
